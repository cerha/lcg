#: ../lib/lcg/export/html.py:505
msgid "Adobe Flash plugin"
msgstr "Adobe Flash plugin"

#. Translators: '%(version)s' is automatically replaced by the required version
#. number.  '%(plugin)s' is replaced by a hypertext link to Adobe Flash plugin
#. download page.
#: ../lib/lcg/export/html.py:501
#, fuzzy, python-format
msgid "Flash %(version)s not detected. Get %(plugin)s %(version)s or later for advanced media playback capabilities."
msgstr "Flash %(version)s nenájdený. Prosím nainštalujte alebo upgradujte %(plugin) %(version)s aby ste mohli využíť rozšírené možnosti prehrávania."

